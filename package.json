{
  "name": "@rebeccastevens/eslint-config",
  "version": "0.0.0-development",
  "description": "My ESLint shareable config.",
  "keywords": [
    "eslint config"
  ],
  "homepage": "https://github.com/RebeccaStevens/eslint-config-rebeccastevens#readme",
  "bugs": {
    "url": "https://github.com/RebeccaStevens/eslint-config-rebeccastevens/issues"
  },
  "repository": "git@github.com:RebeccaStevens/eslint-config-rebeccastevens.git",
  "license": "BSD-3-Clause",
  "author": {
    "name": "Rebecca Stevens",
    "email": "rebecca.stevens@outlook.co.nz"
  },
  "exports": {
    "./common-overrides": {
      "import": "./dist/common-overrides.mjs",
      "require": "./dist/common-overrides.cjs"
    },
    "./modern": {
      "import": "./dist/modern.mjs",
      "require": "./dist/modern.cjs"
    },
    "./script": {
      "import": "./dist/script.mjs",
      "require": "./dist/script.cjs"
    },
    "./test": {
      "import": "./dist/test.mjs",
      "require": "./dist/test.cjs"
    },
    "./typescript": {
      "import": "./dist/typescript.mjs",
      "require": "./dist/typescript.cjs"
    }
  },
  "files": [
    "dist/",
    "package.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "check-format": "prettier --list-different \"./**/*.{md,ts,yml}\"",
    "check-spelling": "cspell --config=.cspell.json \"**/*.{md,ts}\"",
    "cz": "git-cz",
    "format": "prettier --write \"./**/*.{md,ts,yml}\"",
    "generate-eslintrc": "ts-node -P scripts/tsconfig.json -r tsconfig-paths/register scripts/generate-eslintrc.ts",
    "lint": "yarn lint:js && yarn lint:md",
    "lint:js": "yarn generate-eslintrc && eslint .",
    "lint:md": "markdownlint \"**/*.md\" --config=.markdownlint.json --ignore-path=.markdownlintignore",
    "prepare": "husky install",
    "test": "echo no tests"
  },
  "dependencies": {
    "deepmerge-ts": "^4.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^14.0.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.2",
    "@semantic-release/npm": "^9.0.0",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@types/eslint": "^8.4.1",
    "@types/node": "^17.0.15",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "commitizen": "^4.2.4",
    "cspell": "^6.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^3.4.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^5.0.0-beta.14",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^39.2.9",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-sonarjs": "^0.15.0",
    "eslint-plugin-unicorn": "^45.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "markdownlint-cli": "^0.32.2",
    "nyc": "^15.1.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.67.0",
    "rollup-plugin-auto-external": "^2.0.0",
    "semantic-release": "^19.0.2",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^4.0.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "eslint": ">=8.24.0",
    "eslint-import-resolver-typescript": "*",
    "eslint-plugin-eslint-comments": "*",
    "eslint-plugin-functional": ">=5.0.0-beta.14",
    "eslint-plugin-import": "*",
    "eslint-plugin-jsdoc": "*",
    "eslint-plugin-markdown": "*",
    "eslint-plugin-node": "*",
    "eslint-plugin-optimize-regex": "*",
    "eslint-plugin-promise": "*",
    "eslint-plugin-sonarjs": "*",
    "eslint-plugin-unicorn": "*"
  },
  "engines": {
    "node": "^14.17.0 || >=16.0.0"
  }
}
