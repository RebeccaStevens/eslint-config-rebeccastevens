{
  "name": "@rebeccastevens/eslint-config",
  "version": "0.0.0-development",
  "description": "My ESLint shareable config.",
  "keywords": [
    "eslint config"
  ],
  "homepage": "https://github.com/RebeccaStevens/eslint-config-rebeccastevens#readme",
  "bugs": {
    "url": "https://github.com/RebeccaStevens/eslint-config-rebeccastevens/issues"
  },
  "repository": "git@github.com:RebeccaStevens/eslint-config-rebeccastevens.git",
  "license": "BSD-3-Clause",
  "author": {
    "name": "Rebecca Stevens",
    "email": "rebecca.stevens@outlook.co.nz"
  },
  "exports": {
    "default": "./dist/index.mjs",
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs"
  },
  "main": "dist/index.cjs",
  "module": "lib/index.mjs",
  "files": [
    "dist/",
    "package.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c",
    "commit": "cz",
    "generate-eslintrc": "ts-node -P tsconfig.tools.json scripts/generate-eslintrc.ts",
    "prelint": "yarn generate-eslintrc",
    "lint": "eslint .",
    "report-coverage": "codecov -f coverage/lcov.info",
    "test": "jest"
  },
  "dependencies": {
    "deepmerge": "^4.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-typescript": "^8.1.0",
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.0",
    "@semantic-release/npm": "^7.0.9",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@types/eslint": "^7.2.6",
    "@types/jest": "^26.0.19",
    "@types/node": "~12.19.9",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "codecov": "^3.8.1",
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.16.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^3.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsdoc": "^30.7.9",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-optimize-regex": "^1.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-unicorn": "^24.0.0",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.35.1",
    "semantic-release": "^17.3.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "eslint": ">=6.0.0",
    "eslint-import-resolver-typescript": "*",
    "eslint-plugin-eslint-comments": "*",
    "eslint-plugin-functional": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-jsdoc": "*",
    "eslint-plugin-markdown": "*",
    "eslint-plugin-optimize-regex": "*",
    "eslint-plugin-promise": "*",
    "eslint-plugin-simple-import-sort": "*",
    "eslint-plugin-sonarjs": "*",
    "eslint-plugin-unicorn": "*"
  }
}
